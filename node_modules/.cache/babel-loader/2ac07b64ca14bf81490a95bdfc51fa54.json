    name: \"\",\n        count_serves: \"\",\n        category: {\n          _id: \"\",\n          name: \"\",\n          label: \"\",\n        },\n        image: \"\",\n        is_available: true,\n        is_on_sale: false,\n        price: \"\",\n        description: \"\",\n        items:  [],\n      },\n    };\n  },\n  methods: {\n    reset() {\n      this.$refs.form.reset();\n    },\n\n    registerProduct() {\n      const product = this.product;\n      createProduct(product);\n      console.log(\"cadastrando\");\n    },\n    getCategories() {\n      readCategories().then(({data}) => {\n        this.product.items = data.map(item => {\n          return {\n            text: item.name,\n            value:item._id\n          }\n        })\n      });\n    },\n  },\n  mounted() {\n    this.getCategories();\n\n  },\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/components/register"}]}