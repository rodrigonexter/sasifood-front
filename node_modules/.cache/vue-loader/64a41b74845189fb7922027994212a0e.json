label=\"Categoria\"\n          ></v-select>\n\n          <h3>Para quantas pessoas?</h3>\n          <v-radio-group v-model=\"product.count_serves\" column>\n            <v-radio label=\"Serve 1 pessoa\" value=\"Serve 1 pessoa\"></v-radio>\n            <v-radio label=\"Serve 2 pessoas\" value=\"Serve 2 pessoas\"></v-radio>\n          </v-radio-group>\n\n          <h3>Disponibilidade do produto para o público</h3>\n          <v-radio-group v-model=\"product.is_available\" column>\n            <v-radio label=\"Disponível\" value=\"true\"></v-radio>\n            <v-radio label=\"Indisponível\" value=\"false\"></v-radio>\n          </v-radio-group>\n\n          <v-btn color=\"primary\" @click=\"registerProduct()\"> Cadastrar </v-btn>\n        </form>\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n\n<script>\nimport { routes } from \"../../routes.js\";\nimport { createProduct } from \"../../services/api\";\nimport { readCategories } from \"../../services/api\";\n\nexport default {\n  data() {\n    return {\n      itemsAux: \"\",\n      routes,\n      product: {\n        name: \"\",\n        count_serves: \"\",\n        category: {\n          _id: \"\",\n          name: \"\",\n          label: \"\",\n        },\n        image: \"\",\n        is_available: true,\n        is_on_sale: false,\n        price: \"\",\n        description: \"\",\n        items:  [],\n      },\n    };\n  },\n  methods: {\n    reset() {\n      this.$refs.form.reset();\n    },\n\n    registerProduct() {\n      const product = this.product;\n      createProduct(product);\n      console.log(\"cadastrando\");\n    },\n    getCategories() {\n      readCategories().then(({data}) => {\n        this.product.items = data.map(item => {\n          return {\n            text: item.name,\n            value:item._id\n          }\n        })\n      });\n    },\n  },\n  mounted() {\n    this.getCategories();\n\n  },\n};\n</script>\n\n<style>\n</style>"]}]}